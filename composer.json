{
    "name": "golchha21/options",
    "version": "1.0.0",
    "description": "A Laravel package for global key-value options.",
    "keywords": [
        "golchha21",
        "laravel",
        "laravel-package",
        "options",
        "key-value",
        "php",
        "facade",
        "helper functions",
        "blade directives"
    ],
    "homepage": "https://github.com/golchha21/options",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ulhas Vardhan Golchha",
            "email": "vardhans@ulhas.net",
            "homepage": "https://www.ulhas.net",
            "role": "Web Architect"
        }
    ],
    "support": {
        "email": "vardhans@ulhas.net",
        "issues": "https://github.com/golchha21/options/issues"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2.5|^7.3|^8.0.2",
        "laravel/framework": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "files": [
            "src/Helpers/options.php"
        ],
        "psr-4": {
            "Golchha21\\Options\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Golchha21\\Options\\Providers\\ServiceProvider"
            ]
        },
        "aliases": {
            "Option": "Golchha21\\Options\\Facades\\Option"
        }
    }
}
